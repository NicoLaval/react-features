{
	"name": "react-features",
	"version": "0.1.0",
	"dependencies": {
		"axios": "^0.19.0",
		"jsoneditor": "^9.0.2",
		"jsoneditor-react": "^3.0.0",
		"node-sass": "^4.13.0",
		"react": "^16.0.0",
		"react-cache": "^16.6.0-alpha.8af6728",
		"react-dom": "^16.0.0",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.2.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"source-map-explorer": "source-map-explorer 'build/static/js/*.js' --html bundle-report/source-map-explorer.html",
		"analyze": "npm run build && npm run source-map-explorer",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"eslint": "^6.6.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.7.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"prettier": "^1.19.1",
		"source-map-explorer": "^2.4.2"
	}
}
